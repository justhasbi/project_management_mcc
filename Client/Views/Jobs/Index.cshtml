@{
    Layout = "_AdminLayout";
    ViewData["Title"] = "Jobs";
}
<div class="card shadow">
    <div class="card-header">
        <div class="row">
            <p class="h1 col-8 text-primary">Data Jobs</p>
            <div class="col-4"></div>
        </div>
    </div>
    <!-- Table -->
    <div class="card-body">
        <table class="table table-striped table-bordered table-hover table-responsive-md table-responsive-sm table-responsive-lg" id="dataTable">
            <thead>
                <tr>
                    <td>No</td>
                    <td>Employee Id</td>
                    <td>Full Name</td>
                    <td>Job Id</td>
                    <td>Job Name</td>
                    @*<td>Action</td>*@
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>
@section Scripts {
    <script src="~/js/Job.js"></script>
}
